{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "env": {
    "PRISMA_GENERATE_IN_POSTBUILD": "true"
  },
  "github": {
    "enabled": false
  }
}
